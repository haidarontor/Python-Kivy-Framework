<Ellipse>:
    cols:4
    canvas:
        Color:
            rgb: 1,1,1
        Rectangle:
            pos: self.pos
            size: self.size
    RelativeLayout:
        canvas:
            Color:
                rgb: 1,.1,.5
            Ellipse:
                pos: 0,0
                size: self.size
    RelativeLayout:
        canvas:
            Ellipse:
                segments: 5
                pos: 0,0
                size: self.size
    RelativeLayout:
        canvas:
            Ellipse:
                segments: 4
                pos: 0, 0
                size: self.size
    RelativeLayout:
        canvas:
            Ellipse:
                segments: 3
                pos: 0, 0
                size: self.size		